{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Streamlit 앱 디버깅",
      "type": "debugpy",
      "request": "launch",
      "module": "streamlit",
      "args": [
          "run",
          "${file}",
          "--server.port=8501",
          "--server.headless=true"
      ],
      "console": "integratedTerminal",
      "justMyCode": false,
      "env": {
          "PYTHONPATH": "${workspaceFolder}"
      }
  }
  ]
}